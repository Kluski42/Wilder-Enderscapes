{
  "schemaVersion": 1,
  "id": "enderwild",
  "version": "${version}",
  "name": "Ender Wild",
  "description": "Adds consistency between Wilder Wild and Enderscape.",
  "authors": [
    "Wet Noodle"
  ],
  "contributors": [
    "lunarbunten",
    "hivye"
  ],
  "contact": {
    "sources": "https://github.com/Kluski42/Wilder-Enderscapes"
  },
  "license": "CC0-1.0",
  "icon": "assets/enderwild/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.wetnoodle.enderwild.EnderWild"
    ],
    "client": [
      "net.wetnoodle.enderwild.EnderWildClient"
    ],
    "fabric-datagen": [
      "net.wetnoodle.enderwild.EnderWildDataGenerator"
    ]
  },
  "mixins": [
    "enderwild.mixins.json",
    {
      "config": "enderwild.client.mixins.json",
      "environment": "client"
    }
  ],
  "accessWidener": "enderwild.accesswidener",
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": "~1.21.5",
    "java": ">=21",
    "fabric-api": "*",
    "enderscape": ">=1.0.4",
    "wilderwild": ">=4.0.10-mc1.21.5"
  },
  "suggests": {
    "another-mod": "*"
