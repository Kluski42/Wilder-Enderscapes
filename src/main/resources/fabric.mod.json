{
  "schemaVersion": 1,
  "id": "enderwild",
  "version": "${version}",
  "name": "Ender Wild",
  "description": "Adds consistency between Wilder Wild and Enderscape. Currently, it only adds hollowed log variants to the enderscape logs.",
  "authors": [
    "Wet Noodle"
  ],
  "contact": {
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/enderwild/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.wetnoodle.enderwild.EnderWild"
    ],
    "client": [
      "net.wetnoodle.enderwild.EnderWildClient"
    ],
    "fabric-datagen": [
      "net.wetnoodle.enderwild.EnderWildDataGenerator"
    ]
  },
  "mixins": [
    "enderwild.mixins.json",
    {
      "config": "enderwild.client.mixins.json",
      "environment": "client"
    }
  ],
  "accessWidener": "enderwild.accesswidener",
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": "~1.21.5",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}